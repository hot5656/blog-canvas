{
  "rewrites": [
    { "source": "/tools/:path*", "destination": "https://tools.demos.roberthut.com/:path*" },
    { "source": "/", "destination": "/blog-journal" }
  ],
  "headers": [
    {
      "source": "/tools/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
